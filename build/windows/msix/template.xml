<?xml version="1.0" encoding="utf-8"?>
<MsixPackagingToolTemplate
    xmlns="http://schemas.microsoft.com/msix/packaging/msixpackagingtool/template/2022">
    <Settings
        AllowTelemetry="false"
        ApplyACLsToPackageFiles="true"
        GenerateCommandLineFile="true"
        AllowPromptForPassword="false">
    </Settings>
    <Installer
        Path="devboard"
        Arguments=""
        InstallLocation="C:\Program Files\com.funzm\devboard">
    </Installer>
    <PackageInformation
        PackageName="Devboard"
        PackageDisplayName="Devboard"
        PublisherName="CN=com.funzm"
        PublisherDisplayName="com.funzm"
        Version="0.1.0.0"
        PackageDescription="Supercharge Your workflow. The smart clipboard toolkit for developers.">
        <Capabilities>
            <Capability Name="runFullTrust" />
            
        </Capabilities>
        <Applications>
            <Application
                Id="com.funzm.devboard"
                Description="Supercharge Your workflow. The smart clipboard toolkit for developers."
                DisplayName="Devboard"
                ExecutableName="devboard"
                EntryPoint="Windows.FullTrustApplication">
                
            </Application>
        </Applications>
        <Resources>
            <Resource Language="en-us" />
        </Resources>
        <Dependencies>
            <TargetDeviceFamily Name="Windows.Desktop" MinVersion="10.0.17763.0" MaxVersionTested="10.0.19041.0" />
        </Dependencies>
        <Properties>
            <Framework>false</Framework>
            <DisplayName>Devboard</DisplayName>
            <PublisherDisplayName>com.funzm</PublisherDisplayName>
            <Description>Supercharge Your workflow. The smart clipboard toolkit for developers.</Description>
            <Logo>Assets\AppIcon.png</Logo>
        </Properties>
    </PackageInformation>
    <SaveLocation PackagePath="devboard.msix" />
    <PackageIntegrity>
        <CertificatePath></CertificatePath>
    </PackageIntegrity>
</MsixPackagingToolTemplate>
